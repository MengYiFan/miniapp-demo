/**app.wxss**/
.container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 

.button {
  padding: 30rpx 60rpx;
  margin-top: 60rpx;
  width: 100%;
  box-sizing: border-box;
  border: 1rpx solid #f7f7f8;
  text-align: center;
  border-radius: 12rpx;
}
